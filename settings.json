{
    "editor.fontSize": 35,
    "terminal.integrated.fontSize": 26,
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        // "javascript": "node",
        "javascript": "cls && function prompt{\">>\"}  &&  $Host.ui.rawui.foregroundcolor = \"green\" && cd $dir && node $fileName && $Host.ui.rawui.foregroundcolor = \"red\"",
        "javascript-nodemon": "cls && function prompt{\">>\"}  &&  $Host.ui.rawui.foregroundcolor = \"green\" && cd $dir && npx nodemon $fileName && $Host.ui.rawui.foregroundcolor = \"red\"",
        // "java": "cls && function prompt{\">>\"} && $Host.ui.rawui.foregroundcolor = \"cyan\" && cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt && echo 'n",
        "java": "cls && function prompt{\">>\"}  &&  $Host.ui.rawui.foregroundcolor = \"green\" && cd $dir && javac $fileName && java $fileNameWithoutExt && $Host.ui.rawui.foregroundcolor = \"red\"",
        // "java": "cls && cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cls && function prompt{\">>\"}  &&  $Host.ui.rawui.foregroundcolor = \"green\" &&  cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt && $Host.ui.rawui.foregroundcolor = \"red\"",
        "zig": "zig run",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        // "python": "python -u",
        // "python": "cls && function prompt{\">>\"}  &&  $Host.ui.rawui.foregroundcolor = \"green\" && cd $dir && python $fileName && $Host.ui.rawui.foregroundcolor = \"red\"",
        "python": "cls && function prompt{\">>\"}  && cd $dir && python $fileName && $Host.ui.rawui.foregroundcolor = \"red\"",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "npx ts-node",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runghc",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName",
        "mojo": "mojo run",
        "erlang": "escript",
        "spwn": "spwn build",
        "pkl": "cd $dir && pkl eval -f yaml $fileName -o $fileNameWithoutExt.yaml",
        "gleam": "gleam run -m $fileNameWithoutExt"
    },
    "workbench.colorTheme": "GitHub Dark Default",
    "files.exclude": {
        "**/.git": false,
        "**/*.class": true,
        "**/*.exe": true
    },
    "explorer.confirmDelete": false,
    "git.ignoreMissingGitWarning": true,
    "redhat.telemetry.enabled": false,
    "editor.minimap.renderCharacters": false,
    "extensions.autoUpdate": false,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1E1E1EAA"
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "explorer.confirmDragAndDrop": false,
    "java.help.showReleaseNotes": false,
    "files.defaultLanguage": "${activeEditorLanguage}",
    "workbench.startupEditor": "none",
    "liveServer.settings.donotShowInfoMsg": true,
    "emmet.preferences": {},
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "chatgpt.lang": "en",
    "git.confirmSync": false,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "editor.tabSize": 2,
    "editor.codeActionWidget.showHeaders": false,
    "workbench.editor.empty.hint": "hidden",
    "editor.linkedEditing": true,
    "workbench.iconTheme": "material-icon-theme",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "java": false,
        "python": false,
        "typescript": true
    },
    "python.analysis.packageIndexDepths": [
        {
            "name": "sklearn",
            "depth": 2
        },
        {
            "name": "matplotlib",
            "depth": 2
        },
        {
            "name": "scipy",
            "depth": 2
        },
        {
            "name": "django",
            "depth": 2
        },
        {
            "name": "flask",
            "depth": 2
        },
        {
            "name": "fastapi",
            "depth": 2
        }
    ],
    "java.jdt.ls.java.home": "",
    "java.templates.fileHeader": [],
    "java.edit.smartSemicolonDetection.enabled": true,
    "java.templates.typeComment": [],
    "java.import.exclusions": [
        "**/node_modules/**",
        "**/.metadata/**",
        "**/archetype-resources/**",
        "**/META-INF/maven/**"
    ],
    "java.configuration.runtimes": [],
    "powermode.combo.location": "off",
    "powermode.combo.counterEnabled": "hide",
    "powermode.shake.intensity": 1,
    "powermode.shake.enabled": false,
    "powermode.explosions.offset": 0.375,
    "github.copilot.nextEditSuggestions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "powermode.enabled": true,
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.VSIXPackage": "default",
        "*.hdr": "default"
    },
    "roo-cline.allowedCommands": [
        "npm test",
        "npm install",
        "tsc",
        "git log",
        "git diff",
        "git show"
    ],
    "kilo-code.allowedCommands": [
        "git log",
        "git diff",
        "git show",
        "npm test",
        "npm install",
        "tsc",
        "npm run dev"
    ],
    "kilo-code.deniedCommands": [],
    "chat.instructionsFilesLocations": {
        ".github/instructions": true,
        "C:\\Users\\khile\\AppData\\Local\\Temp\\postman-collections-post-response.instructions.md": true,
        "C:\\Users\\khile\\AppData\\Local\\Temp\\postman-collections-pre-request.instructions.md": true,
        "C:\\Users\\khile\\AppData\\Local\\Temp\\postman-folder-post-response.instructions.md": true,
        "C:\\Users\\khile\\AppData\\Local\\Temp\\postman-folder-pre-request.instructions.md": true,
        "C:\\Users\\khile\\AppData\\Local\\Temp\\postman-http-request-post-response.instructions.md": true,
        "C:\\Users\\khile\\AppData\\Local\\Temp\\postman-http-request-pre-request.instructions.md": true
    },
    "roo-cline.deniedCommands": [],
    "chat.mcp.autostart": "newAndOutdated",
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.proseWrap": "never",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.stickyScroll.enabled": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "chat.tools.terminal.autoApprove": {
        "npm run build": true,
        "npm run lint": true,
        "npx": true
    },
    "chat.agent.maxRequests": 100,
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderWhitespace": "none",
    "workbench.statusBar.visible": false,
    "chat.agent.thinkingStyle": "fixedScrolling",
    "chat.agent.todoList": {
        "position": "default"
    },
    "inlineChat.enableV2": true,
    "inlineChat.notebookAgent": true,
    "editor.cursorBlinking": "expand",
    "workbench.settings.applyToAllProfiles": [
        
    ]
}